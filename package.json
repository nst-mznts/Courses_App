{
  "name": "autocode-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "build": "echo \"Dummy build script required for Autocode Compile\"",
    "test": "npm run test:junit",
    "test:local": "jest --reporters=default",
    "test:watch": "jest --watchAll --reporters=default",
    "test:junit": "jest --reporters=jest-junit",
    "test:xunit": "jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "check-format": "prettier --check \"src/**/*.{ts,tsx,scss}\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "redux": "^5.0.1",
    "sass": "^1.89.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hex-rgb": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-junit": "^16.0.0",
    "jest-transform-css": "^6.0.3",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.5.4",
    "vite": "^5.2.0"
  },
  "jest-junit": {
    "suiteName": "junit test results",
    "outputDirectory": ".",
    "outputName": "junit.xml"
  }
}
